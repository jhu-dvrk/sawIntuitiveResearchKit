{
    "state_collectors":
    [
        {
            "component": "io",
            "tables":
            [
                {
                    "sampling": 20,
                    "table": "MTMLRead",
                    "signals": ["measured_js"] // actuators, not joints
                }
                ,
                {
                    "sampling": 20,
                    "table": "MTMRRead",
                    "signals": ["measured_js"] // actuators, not joints
                }
            ]
        }
        ,
        {
            "component": "MTML",
            "tables":
            [
                {
                    "sampling": 5,
                    "table": "StateTable",
                    "signals": ["measured_js", "jaw/measured_js", "measured_cp"] // joints and cartesian
                }
            ]
        }
        ,
        {
            "component": "MTMR",
            "tables":
            [
                {
                    "sampling": 5,
                    "table": "StateTable",
                    "signals": ["measured_js", "jaw/measured_js", "measured_cp"] // joints and cartesian
                }
            ]
        }
    ]
}
